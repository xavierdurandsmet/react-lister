{
  "name": "react-lister",
  "version": "0.0.1",
  "description": "react-lister.io web app",
  "engines": {
    "node": "6.4.0"
  },
  "main": "build/server.js",
  "scripts": {
    "start": "concurrently \"npm run tsc:w\" \"npm run nodemon\" \"npm run webpack:w\" ",
    "nodemon": "nodemon --ignore dist --ignore build/app",
    "postinstall": "tsc && webpack",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "webpack": "webpack --config webpack.config.js",
    "webpack:w": "webpack --config webpack.config.js --colors --watch",
    "deploy": "cd .. && git subtree push --prefix web heroku master"
  },
  "dependencies": {
    "@types/async": "^2.0.32",
    "@types/aws-sdk": "0.0.37",
    "@types/bcrypt": "0.0.30",
    "@types/body-parser": "0.0.33",
    "@types/bootstrap": "^3.3.32",
    "@types/connect-flash": "0.0.31",
    "@types/cookie-parser": "^1.3.30",
    "@types/dotenv": "^2.0.18",
    "@types/es6-promise": "0.0.32",
    "@types/express": "^4.0.33",
    "@types/express-serve-static-core": "^4.0.36",
    "@types/express-session": "0.0.31",
    "@types/jquery": "^2.0.32",
    "@types/mongoose": "^4.5.36",
    "@types/morgan": "^1.7.32",
    "@types/multer": "0.0.32",
    "@types/node": "^6.0.42",
    "@types/passport": "^0.2.31",
    "@types/passport-local": "^1.0.29",
    "@types/react": "^0.14.38",
    "@types/react-bootstrap": "0.0.33",
    "@types/react-dom": "^0.14.17",
    "@types/react-router": "^2.0.37",
    "@types/serve-favicon": "^2.2.28",
    "@types/serve-static": "^1.7.31",
    "async": "^2.0.1",
    "aws-sdk": "^2.6.3",
    "bcrypt": "^0.8.7",
    "body-parser": "^1.15.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "dotenv": "^2.0.0",
    "express": "4.14.0",
    "express-flash": "0.0.2",
    "express-session": "^1.14.1",
    "jade": "1.11.0",
    "jquery": "^3.1.1",
    "mongoose": "^4.6.0",
    "morgan": "^1.7.0",
    "multer": "^1.2.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.6",
    "react-bootstrap-slider": "^1.1.2",
    "react-dom": "^15.3.2",
    "react-router": "^2.8.1",
    "serve-favicon": "^2.3.0",
    "typescript": "2.0.3",
    "webpack": "^1.13.2"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "css-loader": "^0.25.0",
    "nodemon": "^1.10.2",
    "style-loader": "^0.13.1"
  },
